    pos L V -34;
    pos L W -22;
    pos L quotedblright -210;
    pos L quoteright -140;
    pos L v -12;
    pos L w -14;
    pos P comma -160;
    pos P period -100;
    pos f quotedblright 30;
    pos f quoteright 70;
    pos v period -60;
    pos w period -50;
} kern;

